{$R *.dfm}

procedure TForm23.Button1Click(Sender: TObject);
var
  xNumero: double;

begin
  if (xNumero > 80) or (xNumero < 25) or (xNumero = 40)then
    ShowMessage('Este número é premiado pelo algoritmo')

end;

end.
